{
    "manifest_version": 3,
    "name": "Browser Tab Backup Extensions v2",
    "version": "1.0",
    "permissions": ["tabs", "alarms", "favicon", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
      },
    "host_permissions": ["*://*/"]
  }